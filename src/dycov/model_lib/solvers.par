<?xml version="1.0" encoding="UTF-8"?>
<parametersSet xmlns="http://www.rte-france.com/dynawo">
  <!-- IDA order 2 solver -->
  <set id="IDA">
    <par type="INT" name="order" value="{{ida_order}}"/>
    <par type="DOUBLE" name="initStep" value="{{ida_initStep}}"/>
    <par type="DOUBLE" name="minStep" value="{{ida_minStep}}"/>
    <par type="DOUBLE" name="maxStep" value="{{ida_maxStep}}"/>
    <par type="DOUBLE" name="absAccuracy" value="{{ida_absAccuracy}}"/>
    <par type="DOUBLE" name="relAccuracy" value="{{ida_relAccuracy}}"/>
    <par type="DOUBLE" name="minimalAcceptableStep" value="{{ida_minimalAcceptableStep}}"/>
  </set>
  <!-- SIM solver -->
  <set id="SIM">
    <par type="DOUBLE" name="hMin" value="{{sim_hMin}}"/>
    <par type="DOUBLE" name="hMax" value="{{sim_hMax}}"/>
    <par type="DOUBLE" name="kReduceStep" value="{{sim_kReduceStep}}"/>
    <par type="INT" name="maxNewtonTry" value="{{sim_maxNewtonTry}}"/>
    <par type="STRING" name="linearSolverName" value="{{sim_linearSolverName}}"/>
    <par type="DOUBLE" name="fnormtol" value="{{sim_fnormtol}}"/>
    <par type="DOUBLE" name="minimalAcceptableStep" value="{{sim_minimalAcceptableStep}}"/>
  </set>
</parametersSet>
